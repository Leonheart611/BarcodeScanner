<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/transferInputFragment"
        android:name="dynamia.com.barcodescanner.ui.transferstore.transferinput.TransferInputFragment"
        android:label="transfer_input_fragment"
        tools:layout="@layout/transfer_input_fragment">
        <argument
            android:name="transferNo"
            app:argType="string" />
        <action
            android:id="@+id/action_receivingFragment_to_historyInputFragment"
            app:destination="@id/historyInputFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim" />
        <argument
            android:name="barcodeNo"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="transferType"
            app:argType="dynamia.com.barcodescanner.ui.transferstore.TransferType" />
        <argument
            android:name="stockId"
            app:argType="integer"
            android:defaultValue="0" />
        <argument
            android:name="binCode"
            app:argType="string"
            android:defaultValue='""' />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="dynamia.com.barcodescanner.ui.login.LoginFragment"
        android:label="login_fragment"
        tools:layout="@layout/login_fragment">
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="true"
            app:popUpTo="@id/main_navigation"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="dynamia.com.barcodescanner.ui.home.HomeFragment"
        android:label="home_fragment"
        tools:layout="@layout/home_fragment">
        <action
            android:id="@+id/action_homeFragment_to_pickingListFragment"
            app:destination="@id/transferListFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_homeFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="true"
            app:popUpTo="@id/main_navigation"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_homeFragment_to_stockOpnameFragment"
            app:destination="@id/stockOpnameFragment" />
        <action
            android:id="@+id/action_homeFragment_to_checkStockFragment"
            app:destination="@id/checkStockFragment" />
        <action
            android:id="@+id/action_homeFragment_to_binReclassFragment"
            app:destination="@id/binReclassFragment" />
    </fragment>
    <fragment
        android:id="@+id/transferListFragment"
        android:name="dynamia.com.barcodescanner.ui.transferstore.TransferListFragment"
        android:label="transferList_fragment"
        tools:layout="@layout/transferlist_fragment">
        <action
            android:id="@+id/action_transferListFragment_to_transferDetailFragment"
            app:destination="@id/transferDetailFragment" />
        <argument
            android:name="transferType"
            app:argType="dynamia.com.barcodescanner.ui.transferstore.TransferType" />
    </fragment>
    <fragment
        android:id="@+id/transferDetailFragment"
        android:name="dynamia.com.barcodescanner.ui.transferstore.transferdetail.TransferDetailFragment"
        android:label="transfer_detail_fragment"
        tools:layout="@layout/transfer_detail_fragment">
        <argument
            android:name="transferNo"
            app:argType="string" />
        <action
            android:id="@+id/action_transferDetailFragment_to_transferInputFragment"
            app:destination="@id/transferInputFragment" />
        <argument
            android:name="transferType"
            app:argType="dynamia.com.barcodescanner.ui.transferstore.TransferType" />
    </fragment>
    <fragment
        android:id="@+id/historyInputFragment"
        android:name="dynamia.com.barcodescanner.ui.history.HistoryInputFragment"
        android:label="history_input_fragment"
        tools:layout="@layout/history_input_fragment">
        <argument
            android:name="showAll"
            android:defaultValue="false"
            app:argType="boolean" />
        <argument
            android:name="documentNo"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="lineNo"
            android:defaultValue="0"
            app:argType="integer" />
        <argument
            android:name="historyType"
            app:argType="dynamia.com.barcodescanner.ui.history.HistoryType" />
    </fragment>
    <fragment
        android:id="@+id/stockOpnameFragment"
        android:name="dynamia.com.barcodescanner.ui.stockopname.StockOpnameFragment"
        android:label="stock_opname_fragment"
        tools:layout="@layout/stock_opname_fragment" >
        <action
            android:id="@+id/action_stockOpnameFragment_to_historyInputFragment"
            app:destination="@id/historyInputFragment" />
        <action
            android:id="@+id/action_stockOpnameFragment_to_stockOpnameInputFragment2"
            app:destination="@id/stockOpnameInputFragment2" />
        <action
            android:id="@+id/action_stockOpnameFragment_to_stockOpnameDeleteFragment"
            app:destination="@id/stockOpnameDeleteFragment" />
    </fragment>
    <fragment
        android:id="@+id/checkStockFragment"
        android:name="dynamia.com.barcodescanner.ui.checkstock.CheckStockFragment"
        android:label="check_stock_fragment"
        tools:layout="@layout/check_stock_fragment" />
    <fragment
        android:id="@+id/binReclassFragment"
        android:name="dynamia.com.barcodescanner.ui.binreclass.BinReclassFragment"
        android:label="bin_reclass_fragment"
        tools:layout="@layout/bin_reclass_fragment" >
        <action
            android:id="@+id/action_binReclassFragment_to_binreclassDetailFragment"
            app:destination="@id/binreclassDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/binreclassDetailFragment"
        android:name="dynamia.com.barcodescanner.ui.binreclass.detail.BinreclassDetailFragment"
        android:label="binreclass_detail_fragment"
        tools:layout="@layout/binreclass_detail_fragment" >
        <argument
            android:name="binTo"
            app:argType="string" />
        <argument
            android:name="binFrom"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/stockOpnameInputFragment2"
        android:name="dynamia.com.barcodescanner.ui.stockopname.input.StockOpnameInputFragment"
        android:label="stock_opname_input_fragment"
        tools:layout="@layout/stock_opname_input_fragment" >
        <argument
            android:name="id"
            app:argType="integer"
            android:defaultValue="0" />
        <argument
            android:name="barcode"
            app:argType="string" />
        <action
            android:id="@+id/action_stockOpnameInputFragment2_to_historyInputFragment"
            app:destination="@id/historyInputFragment" />
    </fragment>
    <fragment
        android:id="@+id/stockOpnameDeleteFragment"
        android:name="dynamia.com.barcodescanner.ui.stockopname.delete.StockOpnameDeleteFragment"
        android:label="stock_opname_delete_fragment"
        tools:layout="@layout/stock_opname_delete_fragment" />
</navigation>